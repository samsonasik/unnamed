{
    "name": "unnamed/unnamed",
    "description": "Flexible, strong and fast CMS built on top of Zend Framework 2",
    "license": "MIT",
    "type": "project",
    "homepage": "https://bitbucket.org/StanimirDim92/unnamed#readme",
    "authors": [
        {
            "name": "Stanimir Dimitrov",
            "email": "stanimirdim92@gmail.com",
            "homepage": "https://bitbucket.org/StanimirDim92/unnamed#readme",
            "role": "Creator"
        }
    ],
    "keywords": [
        "framework",
        "zf2",
        "Unnamed"
    ],
    "require": {
        "php": ">=5.5",
        "zendframework/zend-authentication": "2.*",
        "zendframework/zend-cache": "2.*",
        "zendframework/zend-captcha": "2.*",
        "zendframework/zend-code": "2.*",
        "zendframework/zend-config": "2.*",
        "zendframework/zend-console": "2.*",
        "zendframework/zend-crypt": "2.*",
        "zendframework/zend-db": "2.*",
        "zendframework/zend-debug": "2.*",
        "zendframework/zend-di": "2.*",
        "zendframework/zend-escaper": "2.*",
        "zendframework/zend-eventmanager": "2.*",
        "zendframework/zend-file": "2.*",
        "zendframework/zend-filter": "2.*",
        "zendframework/zend-form": "2.*",
        "zendframework/zend-http": "2.*",
        "zendframework/zend-hydrator": "2.*",
        "zendframework/zend-i18n": "2.*",
        "zendframework/zend-i18n-resources": "2.*",
        "zendframework/zend-inputfilter": "2.*",
        "zendframework/zend-json": "2.*",
        "zendframework/zend-loader": "2.*",
        "zendframework/zend-log": "2.*",
        "zendframework/zend-mail": "2.*",
        "zendframework/zend-math": "2.*",
        "zendframework/zend-mime": "2.*",
        "zendframework/zend-modulemanager": "2.*",
        "zendframework/zend-mvc": "2.*",
        "zendframework/zend-paginator": "2.*",
        "zendframework/zend-progressbar": "2.*",
        "zendframework/zend-serializer": "2.*",
        "zendframework/zend-servicemanager": "2.*",
        "zendframework/zend-session": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "zendframework/zend-uri": "2.*",
        "zendframework/zend-validator": "2.*",
        "zendframework/zend-version": "2.*",
        "zendframework/zend-view": "2.*",
         "doctrine/doctrine-module": "^0.9.0",
         "doctrine/doctrine-orm-module": "^0.9.1",
         "doctrine/migrations": "^1.1"
    },
    "require-dev": {
        "zendframework/zend-developer-tools": "dev-master",
        "san/san-session-toolbar": "0.*",
        "squizlabs/php_codesniffer": "2.*",
        "phpunit/PHPUnit": "4.*",
        "satooshi/php-coveralls": "dev-master",
        "codeception/codeception": "2.*"
    },
    "autoload": {
        "psr-4": {
          "Application\\": "module/Application/src",
          "Admin\\": "module/Admin/src",
          "Themes\\": "module/Themes/src"
        }
    },
    "suggest": {
       "ext-mcrypt": "for encryption",
       "ext-mbstring": "for proper string encoding",
       "ext-intl": "for translations",
       "ext-gd": "for image manipulation",
       "ext-fileinfo": "for image mimetype detection",
       "ext-pdo": "for database driver",
       "ext-opcache": "for caching"
    },
    "prefer-stable": true
}
